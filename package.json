{
  "name": "as",
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.0",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-stage-2": "~6.22.0",
    "rollup": "~0.41.6",
    "rollup-plugin-babel": "~2.7.1"
  },
  "scripts": {
    "start": "NODE_ENV=development babel-node index.js",
    "build": "NODE_ENV=production rollup --format cjs --config rollup.config.js as.js > lib.js"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "es2015",
          "stage-2"
        ]
      },
      "production": {
        "presets": [
          "stage-2"
        ]
      }
    }
  }
}
